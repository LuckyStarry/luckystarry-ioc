{
  "name": "luckystarry-ioc",
  "version": "1.0.1-0",
  "description": "一个轻量的类似于 ASP.NET MVC 的 IoC 框架",
  "keywords": [
    "ioc"
  ],
  "scripts": {
    "prebuild": "del-cli dist -f",
    "build": "tsc",
    "pretest": "del-cli .nyc_output coverage -f",
    "test": "nyc mocha",
    "testonly": "mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "gulp dev"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "include": [
      "src"
    ],
    "all": true
  },
  "main": "dist/index.js",
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.8",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "gulp": "^5.0.0",
    "mocha": "^11.0.1",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh:git@github.com:LuckyStarry/luckystarry-ioc.git"
  },
  "bugs": {
    "email": "starry@vip.qq.com",
    "url": "https://github.com/LuckyStarry/luckystarry-ioc/issues"
  },
  "author": {
    "name": "SUN BO",
    "email": "starry@vip.qq.com",
    "url": "https://www.luckystarry.com"
  }
}
